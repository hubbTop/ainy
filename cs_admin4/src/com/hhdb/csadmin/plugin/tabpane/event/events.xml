<?xml version="1.0" encoding="UTF-8"?>
<events>
	<event from="com.hhdb.csadmin.plugin.test" to="com.hhdb.csadmin.plugin.tabpane"
		type="CMD">
		<!--一个事件中可以包含一个或者多个属性 -->
		<property name="CMD">
			AddPanelEvent
		</property>
		<property name="TAB_TITLE">
			ceshi
		</property>
		<property name="COMPONENT_ID">
			123
		</property>
		<property name="ICO">
			chart.png
		</property>
	</event>
	<!-- 这是删除面板之后给面板所在插件返回的event格式 -->
	<event from="com.hhdb.csadmin.plugin.tabpane" to="com.hhdb.csadmin.plugin.test"
		type="CMD">
		<property name="CMD">
			RemovePanelEvent
		</property>
		<!--一个事件中可以包含一个或者多个属性 -->
		<property name="COMPONENT_ID">
			123
		</property>
	</event>
	<event from="com.hhdb.csadmin.plugin.attribute" to="com.hhdb.csadmin.plugin.tabpane"
		type="CMD">
		<property name="CMD">
			flushAttributeEvent
		</property>
		<!--一个事件中可以包含一个或者多个属性 -->
	</event>
	<!-- 清空tablepanel里面所有打开的面板 -->
	<event from="com.hhdb.csadmin.plugin.test" to="com.hhdb.csadmin.plugin.table_create"
		type="CMD">
		<property name="CMD">
			CleanEvent
		</property>
	</event>
	<!-- 清空某个面板 -->
	<event from="com.hhdb.csadmin.plugin.test" to="com.hhdb.csadmin.plugin.table_create"
		type="CMD">
		<property name="CMD">
			closePane
		</property>
		<property name="id">
			id
		</property>
	</event>
</events>