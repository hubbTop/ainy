<?xml version="1.0" encoding="UTF-8"?>
<events>
	<!-- 表格导出事件 -->
	<event from="com.hhdb.csadmin.plugin.test" to="com.hhdb.csadmin.plugin.table_importandexport"
		type="CMD">
		<property name="CMD">
			TableExportEvent
		</property>
		<!--一个事件中可以包含一个或者多个属性 -->
		<property name="schemaName">
			public
		</property>
		<property name="databaseName">
			hhdb
		</property>
		<property name="tableName">
			test1
		</property>
	</event>

	<!-- 表格导入事件 -->
	<event from="com.hhdb.csadmin.plugin.test" to="com.hhdb.csadmin.plugin.table_importandexport"
		type="CMD">
		<property name="CMD">
			TableImportEvent
		</property>
		<!--一个事件中可以包含一个或者多个属性 -->
		<property name="schemaName">
			public
		</property>
		<property name="databaseName">
			hhdb
		</property>
		<property name="tableName">
			test1
		</property>
	</event>
</events>



