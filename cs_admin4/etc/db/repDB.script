INSERT INTO SQLBIBLE VALUES(1,2,0,'\u7edf\u8ba1schema\u4e0b\u8868\u683c\u4e2a\u6570','\u000a\u0009\u0009\u0009    \u000a\u0009\u0009\u0009    select count(*) AS \u8868\u683c\u4e2a\u6570 from hh_tables where schemaname=''?1?'';\u000a\u0009\u0009\u0009     \u000a        ')
INSERT INTO SQLBIBLE VALUES(2,2,0,'\u6309\u7167\u8868\u683c\u5927\u5c0f\u7edf\u8ba1\u5f53\u524d\u6a21\u5f0f\u8868\u683c','\u000a\u0009\u0009    SELECT n.nspname as "\u67b6\u6784\u6a21\u5f0f",\u000a  c.relname as "\u540d\u79f0",\u000a  CASE c.relkind WHEN ''r'' THEN ''table'' WHEN ''v'' THEN ''view'' WHEN ''m'' THEN ''materialized view'' WHEN ''i'' THEN ''index'' WHEN ''S'' THEN ''sequence'' WHEN ''s'' THEN ''special'' WHEN ''f'' THEN ''foreign table'' END as "\u7c7b\u578b",\u000a  hh_catalog.hh_get_userbyid(c.relowner) as "\u62e5\u6709\u8005",\u000a  hh_catalog.hh_size_pretty(hh_catalog.hh_table_size(c.oid)) as "\u5927\u5c0f",\u000a  hh_catalog.obj_description(c.oid, ''hh_class'') as "\u63cf\u8ff0"\u000aFROM hh_catalog.hh_class c\u000a     LEFT JOIN hh_catalog.hh_namespace n ON n.oid = c.relnamespace\u000aWHERE c.relkind IN (''r'','''')\u000a      AND n.nspname <> ''hh_catalog''\u000a      AND n.nspname <> ''information_schema''\u000a      AND n.nspname !~ ''^hh_toast''\u000a  AND hh_catalog.hh_table_is_visible(c.oid)\u000aORDER BY 5;\u000a        ')
INSERT INTO SQLBIBLE VALUES(3,2,0,'\u5217\u51fa\u8868\u7a7a\u95f4\u8be6\u60c5','\u000a            SELECT spcname AS "\u540d\u79f0",\u000a\u0009  hh_catalog.hh_get_userbyid(spcowner) AS "\u62e5\u6709\u8005",\u000a\u0009  hh_catalog.hh_tablespace_location(oid) AS "\u6240\u5728\u5730"\u000a,hh_catalog.array_to_string(spcacl, E''\n'') AS "\u5b58\u53d6\u6743\u9650",\u000a\u0009  spcoptions AS "\u9009\u9879",\u000a\u0009  hh_catalog.shobj_description(oid, ''hh_tablespace'') AS "\u63cf\u8ff0"\u000aFROM hh_catalog.hh_tablespace\u000aORDER BY 1;\u000a        ')
