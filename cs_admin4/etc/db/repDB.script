INSERT INTO SQLBIBLE VALUES(22,1,0,'\u6570\u636e\u5e93\u96c6\u5408','\u000a            SELECT\u000a\u0009\u0009\u0009\u0009\u0009datname AS \u6570\u636e\u5e93\u540d,usename AS \u62e5\u6709\u8005,hh_size_pretty(hh_database_size(hh_database.datname))\u000a\u0009\u0009\u0009\u0009\u0009AS \u5927\u5c0f,spcname AS \u8868\u7a7a\u95f4,descr.description AS \u63cf\u8ff0\u000a\u0009\u0009\u0009\u0009\u0009FROM\u000a\u0009\u0009\u0009\u0009\u0009hh_database  LEFT OUTER JOIN hh_shdescription descr ON(hh_database.oid=descr.objoid AND descr.classoid=''hh_database''::regclass),hh_user u,hh_tablespace t \u000a\u0009\u0009\u0009\u0009\u0009WHERE\u000a\u0009\u0009\u0009\u0009\u0009datdba=u.usesysid\u000a\u0009\u0009\u0009\u0009\u0009AND\u000a\u0009\u0009\u0009\u0009\u0009dattablespace=t.oid\u000a\u0009\u0009\u0009\u0009\u0009and datname not in(''template0'',''template1'')\u000a        ')
