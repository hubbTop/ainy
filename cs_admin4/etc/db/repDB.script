INSERT INTO SQLBIBLE VALUES(5,2,0,'\u8868\u7ed3\u6784\u67e5\u8be2','\u000a           SELECT\u000aattname,typname,adsrc\u000a     FROM\u000a           hh_attribute\u000a           INNER JOIN hh_class  ON hh_attribute.attrelid = hh_class.oid\u000a           INNER JOIN hh_type   ON hh_attribute.atttypid = hh_type.oid\u000a           LEFT OUTER JOIN hh_attrdef ON hh_attrdef.adrelid = hh_class.oid AND hh_attrdef.adnum = hh_attribute.attnum\u000a           LEFT OUTER JOIN hh_description ON hh_description.objoid = hh_class.oid AND hh_description.objsubid = hh_attribute.attnum\u000a     WHERE\u000a           hh_attribute.attnum > 0\u000a          AND attisdropped <> ''t''\u000a           AND hh_class.relname= ''?1?''  \u000a     ORDER BY hh_attribute.attnum ;\u000a        ')
INSERT INTO SQLBIBLE VALUES(6,2,0,'\u8868\u7d22\u5f15\u67e5\u8be2','\u000a            \u000a            select t.relname as \u8868\u540d,c.relname as \u7d22\u5f15\u540d \u000afrom (select relname,indexrelid from hh_index i,hh_class c \u000awhere c.relname=''?1?'' and indrelid=c.oid) t,hh_index i,hh_class c\u000awhere t.indexrelid=i.indexrelid and i.indexrelid=c.oid;\u000a    ')
